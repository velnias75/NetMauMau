IDI_ICON1               ICON    DISCARDABLE     "@top_srcdir@/netmaumau.ico"

1 VERSIONINFO
 FILEVERSION 0,0,23,5
 PRODUCTVERSION 0,0,23,5
 FILEFLAGSMASK 0x17L
#ifndef NDEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
			VALUE "CompanyName", "Heiko Schäfer"
            VALUE "FileDescription", "NetMauMau server"
			VALUE "FileVersion", "@PACKAGE_VERSION@"
			VALUE "InternalName", "@PACKAGE_TARNAME@"
            VALUE "LegalCopyright", "© @BUILD_YEAR@ Heiko Schäfer"
            VALUE "OriginalFilename", "nmm-sever.exe"
			VALUE "ProductName", "@PACKAGE_STRING@ server"
			VALUE "ProductVersion", "@PACKAGE_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
